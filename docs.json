{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "maple",
  "name": "Vision Fly AI",
  "colors": {
    "primary": "#16A34A",
    "light": "#07C983",
    "dark": "#1B7B7B"
  },
  "favicon": "/favicon.png",
  "navigation": {
    "tabs": [
      {
        "tab": "Guides",
        "groups": [
          {
            "group": "Get Started",
            "pages": ["introduction", "quickstart", "development"]
          }
        ]
      },
      {
        "tab": "API Reference",
        "groups": [
          {
            "group": "API Documentation",
            "pages": [
              "api-reference/introduction",
              "api-reference/parameters",
              "api-reference/best-practices"
            ]
          },
          {
            "group": "API Examples",
            "pages": ["api-reference/examples"]
          },
          {
            "group": "Endpoints",
            "openapi": {
              "source": "api-reference/openapi.json"
            }
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Documentation",
          "href": "https://docs.visionfly.ai",
          "icon": "book-open-cover"
        },
        {
          "anchor": "Blog",
          "href": "https://visionfly.ai/blog",
          "icon": "newspaper"
        }
      ]
    }
  },

  "logo": {
    "light": "/logo/logo-no-background.svg",
    "dark": "/logo/logo-no-background.svg",
    "href": "https://visionfly.ai"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "mailto:hello@visionfly.ai"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Dashboard",
      "href": "https://dashboard.visionfly.ai"
    }
  },
  "footer": {
    "socials": {
      "x": "https://x.com/mintlify",
      "github": "https://github.com/mintlify",
      "linkedin": "https://linkedin.com/company/mintlify"
    }
  }
}
